{

    "manifest_version": 2,
    "name": "YTBlocker",
    "version": "0.1",
  
    "description": "Block Ads in youtube",
  
    "icons": {
        "48": "icons/border-48.png"
    },
    
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "<all_urls>"
  ],

    "background": {
        "scripts": ["ytblocker.js"]
    }
  
  }